// Enumeration of possible port statuses for ServerObjectFactory
public enum ServerObjectFactoryPortStatus {
    BUSY,
    FREE;

    // Static method to get a PortStatus object by its name
    public static ServerObjectFactoryPortStatus fromString(String name) {
        return valueOf(ServerObjectFactoryPortStatus.class, name);
    }

    // Static method to get an array of all PortStatus objects
    public static ServerObjectFactoryPortStatus[] values() {
        return ServerObjectFactoryPortStatus.values();
    }
}
