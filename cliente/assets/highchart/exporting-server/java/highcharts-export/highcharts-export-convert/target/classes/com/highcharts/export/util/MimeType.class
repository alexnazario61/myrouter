for (MimeType mimeType : MimeType.values()) {
    String extension = mimeType.getExtension();
    String type = mimeType.getType();
    System.out.println(type + " " + (extension != null ? extension : "n/a"));
}

// Or, if you prefer using the ternary operator:
for (MimeType mimeType : MimeType.values()) {
    System.out.println(mimeType.getType() + " " + mimeType.getExtension() != null ? mimeType.getExtension() : "n/a");
}

