<!-- SVG declaration with default namespace -->
<svg xmlns:xlink="http://www.w3.org/1999/xlink" version="1.1" xmlns="http://www.w3.org/2000/svg" width="600" height="400">

  <!-- Describes the creator of the SVG -->
  <desc>Created with @product.name@ @product.version@</desc>

  <!-- Definitions section for reusable elements -->
  <defs>
    <!-- Clip path for the chart area -->
    <clipPath id="highcharts-3">
      <rect fill="none" x="0" y="0" width="313" height="551"></rect>
    </clipPath>
  </defs>

  <!-- Chart background and gridlines -->
  <rect rx="5" ry="5" fill="#FFFFFF" x="0" y="0" width="600" height="400"></rect>
  <g class="highcharts-grid"></g>
  <g class="highcharts-grid"></g>

  <!-- X and Y axis elements -->
  <g class="highcharts-axis"></g>
  <g class="highcharts-axis"></g>

  <!-- Chart series and markers -->
  <g class="highcharts-series-group">
    <g class="highcharts-series highcharts-tracker"></g>
    <g class="highcharts-markers"></g>
  </g>

  <!-- Chart title and legend -->
  <text></text>
  <g class="highcharts-legend"></g>

  <!-- X-axis labels -->
  <g class="highcharts-axis-labels"></g>

  <!-- Y-axis labels -->
  <g class="highcharts-axis-labels"></g>

  <!-- Tooltip -->
  <g class="highcharts-tooltip"></g>

  <!-- Link to Highcharts.com -->
  <text></text>
</svg>
