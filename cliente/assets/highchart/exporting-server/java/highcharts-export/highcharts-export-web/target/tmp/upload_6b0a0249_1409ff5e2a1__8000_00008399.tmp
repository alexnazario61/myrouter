<!-- SVG document root -->
<svg xmlns:xlink="http://www.w3.org/1999/xlink" version="1.1" xmlns="http://www.w3.org/2000/svg" width="600" height="400">
  <!-- Description of the SVG content -->
  <desc>Created with @product.name@ @product.version@</desc>
  <!-- Definitions of reusable graphical elements -->
  <defs>
    <!-- Linear gradient definition -->
    <linearGradient x1="0" y1="0" x2="500" y2="500" gradientUnits="userSpaceOnUse" id="highcharts-4">
      <!-- Gradient stops -->
      <stop offset="0" stop-color="rgb(255, 255, 255)" stop-opacity="1"></stop>
      <stop offset="1" stop-color="rgb(200, 200, 255)" stop-opacity="1"></stop>
    </linearGradient>
    <!-- Clipping path definition -->
    <clipPath id="highcharts-5">
      <!-- Clipping path rectangle -->
      <rect fill="none" x="0" y="0" width="527" height="297"></rect>
    </clipPath>
  </defs>
  <!-- Chart background and grid lines -->
  <rect rx="5" ry="5" fill="#FFFFFF" x="0" y="0" width="600" height="400"></rect>
  <!-- Chart area with gradient fill -->
  <rect rx="0" ry="0" fill="url(#highcharts-4)" x="63" y="40" width="527" height="297"></rect>
  <!-- Grid lines -->
  <g class="highcharts-grid"></g>
  <!-- Axis labels and ticks -->
  <g class="highcharts-axis"></g>
  <!-- Chart series and markers -->
  <g class="highcharts-series-group"></g>
  <!-- Chart title -->
  <text x="300" y="25" class="highcharts-title"></text>
  <!-- Legend -->
  <g class="highcharts-legend"></g>
  <!-- Tooltip -->
  <g class="highcharts-tooltip"></g>
  <!-- Credits -->
  <text x="590" y="395" style="font-family:'lucida grande', 'lucida sans unicode', verdana, arial, helvetica, sans-serif;font-size:9px;cursor:pointer;color:#909090;fill:#909090;">
    <tspan x="590">Highcharts.com</tspan>
  </text>
</svg>
