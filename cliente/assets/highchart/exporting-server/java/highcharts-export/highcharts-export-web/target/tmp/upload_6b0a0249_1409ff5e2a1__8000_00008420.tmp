<svg xmlns="http://www.w3.org/2000/svg" width="600" height="400">
  <!-- Background rectangle -->
  <rect x="0" y="0" width="600" height="400" rx="5" ry="5" fill="#FFFFFF" />

  <!-- Grid lines, x and y-axis, data series, and axis labels are typically generated dynamically using JavaScript in Highcharts library.
       Here, I'll just provide placeholders for those elements. -->
  <g class="highcharts-grid" />
  <g class="highcharts-axis" />
  <g class="highcharts-series-group" />
  <g class="highcharts-axis-labels" />

  <!-- Chart title -->
  <text x="300" y="25" text-anchor="middle" class="highcharts-title">
    <tspan x="300">Chart title</tspan>
  </text>

  <!-- Legend -->
  <g class="highcharts-legend" />

  <!-- Tooltip, typically shown/hidden using JavaScript in Highcharts library -->
  <g class="highcharts-tooltip" style="..." visibility="hidden" />
</svg>
